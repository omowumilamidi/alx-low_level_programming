#include "main.h"
/**
* _strncat - concatenates two strings
* @n: integer n 
* @dest: copy to
* @src: copy from
* Return: pointer to dest
*/
*char *_strncat(char *dest, char *src, int n);
{	
int i;	
int n; 	
i = 0;	
while (dest[i] != '\0')	
{		
i++;	
}	
n = 0;
while (src[n] != '\0')
{
dest[i] = src[n];
i++;
n++;
}
dest[i] = '\0';
return (dest);
}
